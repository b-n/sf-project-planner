{
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
        "console": false,
        "fetch": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "brace-style": [
            2,
        "1tbs",
        {
            "allowSingleLine": true
        }
        ],
        "camelcase": 0,
        "complexity": [
            2,
        11
        ],
        "consistent-return": 0,
        "dot-notation": 0,
        "eol-last": 0,
        "key-spacing": 0,
        "new-cap": 0,
        "no-console": 0,
        "no-constant-condition": 0,
        "no-irregular-whitespace": 0,
        "no-mixed-spaces-and-tabs": [
            2,
        "smart-tabs"
        ],
        "no-multi-spaces": 0,
        "no-script-url": 0,
        "no-shadow": 0,
        "strict": 0,
        "no-trailing-spaces": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": [
            2,
        {
            "vars": "all",
            "args": "none"
        }
        ],
        "no-use-before-define": 0,
        "one-var": [
            2,
        "never"
        ],
        "quotes": [
            2,
        "single"
        ]
    },
    "ecmaFeatures": {
        "modules": true,
        "classes": true
    },
    "parser": "babel-eslint",
    "plugins": []
}
